{
  "version": 2,
  "builds": [
    {
      "src": "index.js",
      "use": "@vercel/node"
    }
  ],
  "routes": [
    {
      "src": "/(.*)",
      "dest": "index.js",
      "methods": ["GET", "POST", "PATCH", "DELETE"],
      "headers": {
        "cache-control": "s-maxage=1, stale-while-revalidate"
      }
    }
  ],
  "functions": {
    "api": {
      "memory": 512,
      "maxDuration": 20
    }
  }
}
